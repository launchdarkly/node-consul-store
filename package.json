{
  "name": "ldclient-node-consul-store",
  "version": "1.1.0",
  "description": "Consul-backed feature store for the LaunchDarkly Node.js SDK",
  "main": "consul_feature_store.js",
  "license": "Apache-2.0",
  "scripts": {
    "test": "jest --ci --forceExit",
    "check-typescript": "node_modules/typescript/bin/tsc",
    "lint": "eslint --format 'node_modules/eslint-formatter-pretty' --ignore-path .eslintignore ."
  },
  "types": "./index.d.ts",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-preset-env": "1.6.1",
    "eslint": "5.8.0",
    "eslint-formatter-pretty": "1.3.0",
    "jest": "23.6.0",
    "jest-junit": "5.2.0",
    "ldclient-node": ">= 5.6.1",
    "node-cache": "^3.2.1",
    "typescript": "3.0.1"
  },
  "jest": {
    "rootDir": ".",
    "testEnvironment": "node",
    "testMatch": [
      "**/*-test.js"
    ],
    "testResultsProcessor": "jest-junit",
    "transformIgnorePatterns": []
  },
  "dependencies": {
    "consul": "0.34.1",
    "node-cache": "4.2.0",
    "winston": "2.4.1"
  },
  "peerDependencies": {
    "ldclient-node": ">= 5.6.1"
  },
  "engines": {
    "node": ">= 0.8.x"
  },
  "bugs": {
    "url": "https://github.com/launchdarkly/node-consul-store/issues"
  },
  "homepage": "https://github.com/launchdarkly/node-consul-store"
}
